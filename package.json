    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "ajv": "^6.12.6",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.7",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "npx": "^10.2.2",
