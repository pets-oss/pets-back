    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fsevents": "^2.3.1",
    "graphql": "^15.4.0",
    "graphql-constraint-directive": "^2.1.1",
    "graphql-tools": "^7.0.2",
    "lodash": "^4.17.20",
    "pg": "^8.5.1",
    "snakecase-keys": "^3.2.1",
    "sql-bricks-postgres": "^0.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
